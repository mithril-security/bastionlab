    "new_rdf = (\n",
    "    df.select(\n",
    "        pl.when(pl.col(\"Sex\") == \"M\")\n",
    "        .then(\"male\")\n",
    "        .when(pl.col(\"Sex\") == \"m\")\n",
    "        .then(\"male\")\n",
    "        .when(pl.col(\"Sex\") == \"m \")\n",
    "        .then(\"male\")\n",
    "        .when(pl.col(\"Sex\") == \"F\")\n",
    "        .then(\"female\")\n",
    "        .otherwise(pl.col(\"Sex\"))\n",
    "    )\n",
    "    .collect()\n",
    "    .fetch()\n",
    ")\n",
    "new_rdf"
